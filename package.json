{
  "name": "erp",
  "version": "0.1.0",
  "description": "A lila project.",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"./**/*.{js,jsx,vue,css,less,scss,sass,html,md,json}\"",
    "eslint": "eslint \"./**/*.js\"",
    "eslint:fix": "eslint \"./**/*.js\" --fix",
    "eslint-react": "eslint \"./**/*.jsx\" -c .eslintrc.react.js",
    "eslint-react:fix": "eslint \"./**/*.jsx\" -c .eslintrc.react.js --fix",
    "eslint-vue": "eslint \"./**/*.vue\" -c .eslintrc.vue.js",
    "eslint-vue:fix": "eslint \"./**/*.vue\" -c .eslintrc.vue.js --fix",
    "stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue}\"",
    "stylelint:fix": "stylelint \"./**/*.{css,less,scss,sass,vue}\" --fix",
    "flow": "flow",
    "flow:check": "flow check",
    "test": "jest",
    "test-watch": "jest --watch --notify",
    "test-cover": "jest --coverage",
    "coverage": "npm run test-cover && opn coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.12.0",
    "core-js": "^2.6.9",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-vue": "^5.2.2",
    "flow-bin": "^0.94.0",
    "fs-extra": "^8.1.0",
    "gulp-qiniu": "https://github.com/senntyou/gulp-qiniu.git#39e6731",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "js-base64": "^2.5.1",
    "less": "^3.9.0",
    "lila-bin": "^0.8.0",
    "lila-core": "^0.8.0",
    "lila-tasks": "^0.8.0",
    "lila-webpack": "^0.8.1",
    "lila-webpack-config": "^0.8.0",
    "lint-staged": "^8.1.0",
    "moment-locales-webpack-plugin": "^1.0.7",
    "opn-cli": "^4.0.0",
    "prettier": "^1.15.3",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "stylelint": "^9.9.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "//lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.jsx": [
      "eslint -c .eslintrc.react.js --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.vue": [
      "eslint -c .eslintrc.vue.js --fix",
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{css,less,scss,sass}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{html,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,vue,css,less,scss,sass,html,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.3.5",
    "@senntyou/canvas2image": "^0.0.1",
    "@senntyou/shortcut.css": "^0.1.0",
    "antd": "^3.8.4",
    "bootstrap": "^3.3.7",
    "bootstrap-datepicker": "^1.8.0",
    "bootstrap-datetime-picker": "^2.4.4",
    "bootstrap-datetimepicker": "^0.0.7",
    "bootstrap-select": "^1.13.5",
    "bootstrap-slider": "^10.3.2",
    "bootstrap-switch": "3.3.4",
    "chart.js": "^2.7.3",
    "clipboard": "^2.0.4",
    "clone": "^2.1.2",
    "colors.css": "^3.0.0",
    "element-ui": "^2.6.1",
    "events": "^3.0.0",
    "font-awesome": "^4.7.0",
    "handlebars": "^4.0.12",
    "img-vuer": "^0.17.2",
    "jquery": "^3.3.1",
    "jquery-confirm": "3.3.2",
    "jquery-ui": "^1.12.1",
    "jquery.cookie": "^1.4.1",
    "jquery.nicescroll": "^3.7.6",
    "js-cookie": "^2.2.0",
    "json-refactor": "^1.0.0",
    "juicer": "^0.6.15",
    "lunar-calendar": "^0.1.4",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.0",
    "orchids": "^1.0.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "reqwest": "^2.0.5",
    "see-ajax": "^2.0.0",
    "see-fetch": "^1.0.3",
    "see-view": "^1.0.0",
    "spinkit": "^1.2.5",
    "swiper": "^3.4.2",
    "tippy.js": "^3.2.0",
    "toastr": "^2.1.4",
    "underscore": "^1.9.1",
    "vconsole": "^3.2.0",
    "video.js": "^7.4.1",
    "vue": "^2.6.7",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  }
}
